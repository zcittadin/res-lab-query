<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="255.0" prefWidth="280.0" styleClass="myGradient" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.servicos.estatica.resicolor.controller.ConfigChartConsultaController">
   <children>
      <CheckBox fx:id="chkMarcadores" layoutX="96.0" layoutY="37.0" mnemonicParsing="false" onAction="#toggleMark" text="Marcadores" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </CheckBox>
      <Button fx:id="btOk" layoutX="105.0" layoutY="181.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="70.0" text="Ok">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <CheckBox fx:id="chkReator" layoutX="96.0" layoutY="77.0" mnemonicParsing="false" onAction="#toggleReator" text="Visualizar reator" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="chkCaldeira" layoutX="96.0" layoutY="119.0" mnemonicParsing="false" onAction="#toggleCaldeira" text="Visualizar caldeira" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
